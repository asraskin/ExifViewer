					<menuitem id="history-loc-7" label="7" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickLocalHistory(this.label)"/>
					<menuitem id="history-loc-8" label="8" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickLocalHistory(this.label)"/>
