					<menuitem id="history-rem-2" label="2" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickRemoteHistory(this.label)"/>
					<menuitem id="history-rem-3" label="3" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickRemoteHistory(this.label)"/>
					<menuitem id="history-rem-4" label="4" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickRemoteHistory(this.label)"/>
					<menuitem id="history-rem-5" label="5" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickRemoteHistory(this.label)"/>
					<menuitem id="history-rem-6" label="6" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickRemoteHistory(this.label)"/>
					<menuitem id="history-rem-7" label="7" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickRemoteHistory(this.label)"/>
					<menuitem id="history-rem-8" label="8" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickRemoteHistory(this.label)"/>
					<menuitem id="history-rem-9" label="9" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickRemoteHistory(this.label)"/>
					<menuitem id="history-rem-10" label="10" hidden="true" persist="label hidden" oncommand="AlanSRaskin.ExifViewer.Moz.pickRemoteHistory(this.label)"/>
